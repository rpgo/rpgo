{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "dev-master#074160019c547154a77caaeab692e0d39b757f4a",
    "rhumsaa/uuid": "^2.8@stable"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.1@stable",
    "behat/behat": "^3.0@stable",
    "fzaninotto/faker": "^1.4@stable"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Rpgo\\": "app/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
